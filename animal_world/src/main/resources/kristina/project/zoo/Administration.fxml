<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0"
            style="-fx-background-color: #000000;"
            xmlns="http://javafx.com/javafx/15"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="kristina.project.zoo.controllers.Administration">



        <ImageView >
        <Image url="@images/backmenuadm.jpg" />
        </ImageView>
        <Button fx:id="OpenTables" layoutX="14.0" layoutY="14.0" prefHeight="100.0" prefWidth="240.0"  text="Открыть таблицы" textAlignment="CENTER">
            <font>
                <Font name="Times New Roman Bold" size="20.0" />
            </font>
        </Button>
        <Button fx:id="MakeRequest" layoutX="14.0" layoutY="124.0" prefHeight="100.0" prefWidth="240.0"  text="Сделать запрос" textAlignment="CENTER">
         <font>
            <Font name="Courier New" size="20.0" />
         </font>
        </Button>
        <Button fx:id="ChangeTables" layoutX="14.0" layoutY="234.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="240.0"  text="Удалить данные" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Courier New" size="20.0" />
         </font>
        </Button>
    <Button fx:id="backButton" layoutX="14.0" layoutY="344.0" prefHeight="100.0" prefWidth="240.0"  text="Назад" textAlignment="CENTER" >
        <font>
            <Font name="Courier New" size="20.0" />
        </font>
    </Button>
</AnchorPane>
